<odoo>
  <record id="view_product_template_form_inherit_autoparts" model="ir.ui.view">
    <field name="name">product.template.form.inherit.autoparts</field>
    <field name="model">product.template</field>
    <field name="inherit_id" ref="product.product_template_only_form_view"/>
    <field name="arch" type="xml">

      <!-- Add 'is_autoparts' field below the product_tooltip field -->
      <xpath expr="//field[@name='product_tooltip']" position="after">
        <field name="is_autoparts"/>
      </xpath>

    </field>
  </record>
</odoo>